    "eslint-config-google": "^0.14.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
